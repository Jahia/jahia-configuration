<content jcr:primaryType="jnt:systemNode"

         xmlns:jcr="http://www.jcp.org/jcr/1.0"
         xmlns:j="http://www.jahia.org/jahia/1.0"
         xmlns:jnt="http://www.jahia.org/jahia/nt/1.0">

    <shared jcr:primaryType="jnt:systemNode" jcr:mixinTypes="jmix:accessControlled">

        <j:acl jcr:primaryType="jnt:acl">
            <GRANT_jcr_read_live_g_users j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:users"
                                    j:privileges="jcr:read_live" j:aceType="GRANT"/>
            <GRANT_jcr_read_g_users j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:users"
                                     j:privileges="jcr:read" j:aceType="GRANT"/>
            <GRANT_jcr_write_g_users j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:users"
                                     j:privileges="jcr:write" j:aceType="GRANT"/>
            <!--<DENY_g_users j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:users" j:privileges="" j:aceType="DENY"/>-->
        </j:acl>

        <files jcr:primaryType="jnt:folder"/>
        <mashups jcr:primaryType="jnt:portletList"/>
        <portalComponents jcr:primaryType="jnt:contentList"/>
        <contents jcr:primaryType="jnt:contentList"/>
        <conversions jcr:primaryType="jnt:contentList"/>
    </shared>

    <users jcr:primaryType="jnt:usersFolder" j:usersFolderSkeleton="user.xml">
        <ROOT_NAME_PLACEHOLDER jcr:primaryType="jnt:user" j:password="@ROOT_PASSWORD@" j:external="false" jcr:uuid="b32d306a-6c74-11de-b3ef-001e4fead50b">
            <files jcr:primaryType="jnt:folder">
                <private jcr:primaryType="jnt:folder" jcr:mixinTypes="jmix:accessControlled">
                    <j:acl jcr:primaryType="jnt:acl">
                        <DENY_jcr_read_live_g_guest j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:guest"
                                               j:privileges="jcr:read_live" j:aceType="DENY"/>
                        <DENY_jcr_read_g_guest j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:guest"
                                               j:privileges="jcr:read" j:aceType="DENY"/>
                        <DENY_jcr_write_g_guest j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:guest"
                                                j:privileges="jcr:write" j:aceType="DENY"/>
                        <DENY_jcr_modifyAccessControl_g_guest j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:guest"
                                                j:privileges="jcr:modifyAccessControl" j:aceType="DENY"/>
                        <DENY_jcr_write_live_g_guest j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:guest"
                                                j:privileges="jcr:write_live" j:aceType="DENY"/>
                    </j:acl>

                    <imports jcr:primaryType="jnt:importDropBox"/>
                </private>
            </files>

            <mashups jcr:primaryType="jnt:portletList"/>

            <contents jcr:primaryType="jnt:contentList"/>

            <preferences jcr:primaryType="jnt:preferences" jcr:mixinTypes="jmix:accessControlled">
                <j:acl jcr:primaryType="jnt:acl">
                    <DENY_jcr_read_live_g_guest j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:guest"
                                           j:privileges="jcr:read_live" j:aceType="DENY"/>
                    <DENY_jcr_read_g_guest j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:guest"
                                           j:privileges="jcr:read" j:aceType="DENY"/>
                    <DENY_jcr_write_g_guest j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:guest"
                                            j:privileges="jcr:write" j:aceType="DENY"/>
                    <DENY_jcr_modifyAccessControl_g_guest j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:guest"
                                            j:privileges="jcr:modifyAccessControl" j:aceType="DENY"/>
                    <DENY_jcr_write_live_g_guest j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:guest"
                                            j:privileges="jcr:write_live" j:aceType="DENY"/>
                </j:acl>
            </preferences>
        </ROOT_NAME_PLACEHOLDER>
        <guest jcr:primaryType="jnt:user" j:password="" j:external="false"/>
    </users>

    <groups jcr:primaryType="jnt:groupsFolder">
        <guest jcr:primaryType="jnt:group" j:hidden="false">
            <j:members jcr:primaryType="jnt:members"/>
        </guest>
        <users jcr:primaryType="jnt:group" j:hidden="false">
            <j:members jcr:primaryType="jnt:members"/>
        </users>
        <administrators jcr:primaryType="jnt:group" j:hidden="false">
            <j:members jcr:primaryType="jnt:members">
                <root jcr:primaryType="jnt:member" j:member="b32d306a-6c74-11de-b3ef-001e4fead50b"/>
            </j:members>
        </administrators>
    </groups>
    
    <workflowrules jcr:primaryType="jnt:workflowRules"/>

    <sites jcr:primaryType="jnt:virtualsitesFolder"
           j:virtualsitesFolderSkeleton="site.xml,/modules/roles/site-roles.xml"/>

    <imports jcr:primaryType="jnt:importDropBox"/>
    <referencesKeeper jcr:primaryType="jnt:referencesKeeper"/>

    <mounts jcr:primaryType="jnt:systemNode" jcr:mixinTypes="jmix:accessControlled">
        <j:acl jcr:primaryType="jnt:acl">
            <GRANT_jcr_read_g_users j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:users"
                                    j:privileges="jcr:read" j:aceType="GRANT"/>
            <GRANT_jcr_write_g_users j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:users"
                                     j:privileges="jcr:write" j:aceType="GRANT"/>
        </j:acl>
    </mounts>
    <portletdefinitions jcr:primaryType="jnt:portletDefinitions"/>
    <categories jcr:primaryType="jnt:category" jcr:mixinTypes="jmix:accessControlled">
        <j:acl jcr:primaryType="jnt:acl">
            <GRANT_jcr_write_g_administrators j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:administrators"
                                    j:privileges="jcr:write" j:aceType="GRANT"/>
            <GRANT_jcr_modifyAccessControl_g_guest j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:administrators"
                                    j:privileges="jcr:modifyAccessControl" j:aceType="GRANT"/>
            <GRANT_jcr_write_live_g_administrators j:protected="false" jcr:primaryType="jnt:ace" j:principal="g:administrators"
                                    j:privileges="jcr:write_live" j:aceType="GRANT"/>
        </j:acl>
    </categories>

    <templatesSet jcr:primaryType="jnt:templatesSets"/>

    <systemwrapper jcr:primaryType="jnt:template"  j:template="systemwrapper" j:applyOn="jmix:list"/>

</content>