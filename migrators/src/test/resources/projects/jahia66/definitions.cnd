[genericmix:pictoMixin] > jmix:templateMixin mixin
 extends=jmix:navMenuItem
- picto (weakreference, picker[type='image']) < jnt:file

[genericmix:navMenuLayout] mixin
 extends=jmix:navMenuItem
 itemtype=content
 - layoutType (string, choicelist[navMenuLayoutInitializer,resourceBundle]) = 'fontAwesomeInlineIcon' autocreated < 'fontAwesomeInlineIcon', 'picto'

[genericmix:siteLogo] mixin
 extends=jnt:virtualsite
 itemtype=content
- logo (weakreference, picker[type='image']) < jnt:file
- baseline (string) i18n

[genericmix:alternateTitle] mixin
 extends=jmix:navMenuItem
 itemtype=content
- alternateTitle (string) i18n

[genericmix:intro] mixin
 extends=jmix:navMenuItem
 itemtype=content
- intro (string, richtext[ckeditor.customConfig='$context/modules/jahiaone-templates/javascript/ckconfig.js']) i18n

[genericmix:hidePage] mixin
 extends=jmix:navMenuItem
 itemtype=content
 - value (string) = "This page will be hidden in the side menu." protected

[genericmix:listOrderedWithStyles]  > jmix:templateMixin mixin
 extends = jmix:list,jnt:area
 itemtype = layout
 - ulClassName (string) indexed=no
 - liClassName (string) indexed=no

[genericnt:navbar]  > jnt:content, genericmix:genericComponents, jmix:studioOnly, jmix:navMenuComponent, mix:title
- logo (weakreference, picker[type='image']) < jnt:file

[genericnt:navDropdown]  > jnt:content, genericmix:genericComponents, jmix:studioOnly, jmix:navMenuComponent, mix:title
- baseLevel (long) mandatory

[genericnt:navSiblings]  > jnt:content, genericmix:genericComponents, jmix:studioOnly, jmix:navMenuComponent, mix:title
- baseLevel (long) mandatory

[genericnt:navside]  > jnt:content, genericmix:genericComponents, jmix:studioOnly, jmix:navMenuComponent
[genericnt:lang]  > jnt:content, genericmix:genericComponents, jmix:studioOnly
[genericnt:login]  > jnt:content, genericmix:genericComponents, jmix:studioOnly
[genericnt:searchForm]  > jnt:simpleSearchForm, genericmix:genericComponents, jmix:studioOnly
[genericnt:searchResultsInfo]  > jnt:content, jmix:bindedComponent, genericmix:genericComponents, jmix:studioOnly


[genericmix:bootstrapRowPredefined] > jmix:templateMixin  mixin
 extends=genericnt:bootstrapRow,genericnt:columnsLayout
- layout (string, choicelist[resourceBundle,moduleImage='png']) = '4_8' autocreated < '2_10','3_9','4_8','4_4_4','3_6_3','3_3_3_3','6_6','8_4', '9_3','10_2','12','0'

[genericmix:bootstrapRowCustom] > jmix:templateMixin  mixin
 extends=genericnt:bootstrapRow,genericnt:columnsLayout
- columns (string) = 'span4, span2 offset1, span3 offset2'

[genericmix:advancedColumnProperties] mixin
 extends=genericnt:columnsLayout
 itemtype=content
  - sectionElement (string, choicelist[resourceBundle]) = 'section' autocreated < 'section', 'article', 'header', 'footer', 'aside', 'nav', 'div'
  - sectionCssClass (string)
  - containerType (string, choicelist[resourceBundle]) = 'fluid' autocreated < 'fixed', 'fluid'
  - containerCssClass (string)

[genericnt:bootstrapRow] > jnt:content
- rowType (string, choicelist[rowInitializer,resourceBundle]) = 'predefined' autocreated < 'predefined', 'custom'
+ * (jmix:droppableContent) = jmix:droppableContent version

[genericnt:bootstrapContainer] > jnt:content, genericmix:genericLayoutComponents, jmix:studioOnly
orderable
 - containerType (string, choicelist[resourceBundle]) = 'fluid' autocreated < 'fixed', 'fluid'
 - cssClassName (string)
 + * (genericnt:bootstrapRow)

[genericnt:section] > jnt:content, genericmix:genericLayoutComponents, jmix:studioOnly
 orderable
  - sectionElement (string, choicelist[resourceBundle]) = 'section' autocreated < 'section', 'article', 'header', 'footer', 'aside', 'nav','div'
  - cssClassName (string)
 + * (jmix:droppableContent) = jmix:droppableContent version

[genericmix:text] mixin
 - text (string, richtext[ckeditor.customConfig='$context/modules/jahiaone-templates/javascript/ckconfig.js']) i18n
[genericnt:text] > jnt:content, genericmix:text, genericmix:genericComponents


[genericnt:columnsLayout] > jnt:content, genericmix:genericComponents
 orderable
  - rowType (string, choicelist[rowInitializer,resourceBundle]) = 'predefined' autocreated < 'predefined', 'custom'
 + * (jmix:droppableContent) = jmix:droppableContent version
 + * (genericnt:bootstrapRow)


[genericmix:internalLink] > jmix:templateMixin  mixin
   extends=genericmix:linkOnly
   - internalLink (weakreference, picker[type='page']) < jnt:page


[genericmix:externalLink] > jmix:templateMixin  mixin
   extends=genericmix:linkOnly
   - linkTitle (string) i18n
   - externalLink (string) = 'http://' i18n

[genericmix:linkOnly] mixin
 - linkType (string, choicelist[linkInitializer,resourceBundle]) = 'internal' < 'internal', 'external'

[genericnt:link] > jnt:content, genericmix:linkOnly

[genericmix:image] mixin
 - image (weakreference, picker[type='image']) < jnt:file


// "TILT" is (T)itle, (I)mage, (L)ink, (T)ext
[genericmix:tilt] > mix:title, genericmix:linkOnly, genericmix:image, genericmix:text mixin

[genericnt:carouselItem] > jnt:content, mix:title, genericmix:linkOnly
 - lead (string) i18n mandatory
 - image (weakreference, picker[type='image']) i18n < jnt:file

[genericnt:carousel] > jnt:contentList, genericmix:genericComponents
orderable
+ * (genericnt:carouselItem)

[genericnt:clickableImage] > jnt:content, genericmix:linkOnly, genericmix:image

[genericmix:clickableImagesCss] mixin
 extends=genericnt:clickableImages
 itemtype=layout
  - cssClass (string)

[genericnt:clickableImages] > jnt:contentList, genericmix:genericComponents, mix:title
orderable
+ * (genericnt:clickableImage)

[genericnt:teaser]  > jnt:content, genericmix:genericComponents, genericmix:tilt
[genericnt:footerLinks]  > jnt:content, genericmix:genericComponents, mix:title
 + * (genericnt:link)
[genericnt:advertisement]  > jnt:content, genericmix:genericComponents, genericmix:tilt

[genericnt:demoBox]  > jnt:content, genericmix:genericComponents
- recipients (string) mandatory < "(\\b[\\w.%-]+@[-.\\w]+\\.[A-Za-z]{2,4}\\b)(\\s*,\\s*\\b[\\w.%-]+@[-.\\w]+\\.[A-Za-z]{2,4}\\b)*"
- background (weakreference, picker[type='image']) < jnt:file
- thanksMessageKey (string)

[genericnt:findPartnerBox]  > jnt:content, genericmix:genericComponents
 - partnerList (weakreference, picker[type='editorial']) < jnt:contentList

[genericnt:tabs] > jnt:tabularList, genericmix:genericComponents, jmix:manuallyOrderable
  orderable
  - tabsPosition (string, choicelist[resourceBundle]) = 'top' indexed=no mandatory < 'left', 'top', 'right', 'below'
  - fadeIn (boolean) = true autocreated mandatory indexed=no

[genericnt:social]  > jnt:content, genericmix:genericComponents

[genericnt:btnGroup]  > jnt:content, genericmix:genericComponents, mix:title
 + * (genericnt:link)

[genericnt:contactForm] > jnt:content, genericmix:genericComponents
 - recipient (string) mandatory < "\\b[\\w.%-]+@[-.\\w]+\\.[A-Za-z]{2,4}\\b"
 - subjectPattern (string)

[genericnt:contactButton]  > genericnt:btnGroup
 + contactForm (genericnt:contactForm) = genericnt:contactForm

[genericnt:breadcrumb]  > jnt:content, genericmix:genericComponents, jmix:studioOnly
[genericnt:metadatas]  > jnt:content, genericmix:genericComponents, jmix:studioOnly
[genericnt:pageTitle]  > jnt:content, genericmix:genericComponents
[genericnt:feature]  > jnt:content, mix:title, genericmix:text, genericmix:image, genericmix:genericComponents
 - image (weakreference, picker[type='image']) < jnt:file
 - imageHover (weakreference, picker[type='image']) i18n < jnt:file
 - imageFancy (weakreference, picker[type='image']) i18n < jnt:file
 + * (genericmix:image)

[genericnt:sitemap]  > jnt:content, genericmix:genericComponents

[genericmix:calendarEntry] > genericmix:tilt
 - date (date) = now()

[genericnt:news] > jnt:content, genericmix:genericComponents, genericmix:calendarEntry

[genericnt:event] > jnt:content, genericmix:genericComponents, genericmix:calendarEntry
 - dateEnd (date) mandatory
 - type (string,choicelist[resourceBundle]) = 'event' autocreated facetable < 'event','webinar'
 - speaker (weakreference, picker[type='page']) mandatory < jnt:page
 - room (string)
 - speaker2 (weakreference, picker[type='page']) < jnt:page
 - image2 (weakreference, picker[type='image']) < jnt:file
 - audience (weakreference, category[autoSelectParent=false]) multiple facetable
 - keypoint (weakreference, category[autoSelectParent=false]) multiple facetable

[genericnt:buzz] > jnt:content, genericmix:genericComponents, genericmix:calendarEntry
 - author (string)

[genericnt:lastNews] > jnt:content, genericmix:genericComponents, jmix:list
 - filter (weakreference, category[autoSelectParent=false])
 - limit (long) = 10 indexed=no mandatory
 - layout (string, choicelist[resourceBundle]) = 'small' autocreated < 'small', 'medium'

[genericmix:client] mixin
 - shortDesc (string, richtext) i18n
 - longDesc  (string, richtext) i18n
 - url (string)
 - logo (weakreference, picker[type='image'])
 - partnerFrom (date)
 - countries (string,choicelist[country]) multiple facetable

[genericnt:partner] > jnt:content, mix:title, genericmix:client, genericmix:genericComponents
 - partnerFrom (date)
 - types (string, choicelist[resourceBundle]) multiple facetable < authorized,gold,rdcenter,silver,certified

[genericnt:customer] > jnt:content, mix:title, genericmix:client, genericmix:genericComponents
- clientFrom (date)
- sector (string, choicelist[resourceBundle]) facetable < aerospace,agriculture,apparel,bank,chemicals,construction,consulting,education,entertainment,environmental,government,healthcare,hospitality,insurance,InternationalOrganization,legal,luxuryGoods,manufacturing,media,notForProfit,pensionServices,retail,services,shipping,technology,telecommunications,transportation,utilities,food
- types (string,choicelist[resourceBundle]) multiple facetable < intranet,portal,corporateWebSite,ecommerceSite,extranet,catalog,multisite

[genericnt:zoomingImage] > jnt:content
 - image (weakreference, picker[type='image']) i18n mandatory < jnt:file
 - imageBig (weakreference, picker[type='image']) i18n mandatory < jnt:file

[genericnt:zoomingImages] > jnt:contentList, genericmix:genericComponents
orderable
 - layout (string, choicelist[resourceBundle]) = '2col' autocreated < '2col', '3col'
+ * (genericnt:zoomingImage)

[genericnt:gallery] > jnt:content, genericmix:genericComponents
 - folder (weakreference, picker[type='folder']) i18n mandatory < jnt:folder

[genericnt:pager] > jnt:pager
 - paginationSize (string,choicelist[resourceBundle]) indexed=no < '', 'pagination-large', 'pagination-small', 'pagination-mini'
 - paginationAlignment (string,choicelist[resourceBundle]) indexed=no < '', 'pagination-centered', 'pagination-right'

[genericnt:countdown] > jnt:content, genericmix:genericComponents,mix:title
[genericnt:banner] > jnt:content, genericmix:genericComponents,genericmix:text,genericmix:image,genericmix:linkOnly
- bgimage (weakreference, picker[type='image']) i18n mandatory < jnt:file
